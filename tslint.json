{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "jsx-wrap-multiline": false,
    "quotemark": [true, "single", "jsx-double"],
    "jsx-no-lambda": false,
    "member-access": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "no-console": false
  }
}
